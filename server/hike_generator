-- Table: Huts
DROP TABLE IF EXISTS Huts;

CREATE TABLE Huts (
    idHut       INTEGER PRIMARY KEY
                        NOT NULL,
    nameHut     TEXT    NOT NULL
                        REFERENCES Points (nameLocation) 
                        UNIQUE,
    phone       TEXT    NOT NULL,
    email       TEXT    NOT NULL,
    web_site    TEXT,
    description TEXT    NOT NULL
);


