-- Table: Users
DROP TABLE IF EXISTS Users;

CREATE TABLE Users (
    email        TEXT NOT NULL
                      UNIQUE,
    password     TEXT NOT NULL,
    role         TEXT NOT NULL,
    name         TEXT NOT NULL,
    lastname     TEXT NOT NULL,
    phone_number TEXT NOT NULL,
    salt         TEXT NOT NULL,
    PRIMARY KEY (
        email
    )
);

