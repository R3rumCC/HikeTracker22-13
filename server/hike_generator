    start_point      INTEGER NOT NULL,
    end_point        INTEGER NOT NULL,
    reference_points TEXT,
    description      TEXT    NOT NULL,
    gpx_track        TEXT    NOT NULL,
    PRIMARY KEY (
        title
    )
);

